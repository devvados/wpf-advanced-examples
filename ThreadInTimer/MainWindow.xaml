        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThreadInTimer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="106" Margin="106,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="192" TextChanged="textBox_TextChanged"/>
